    "build:watch": "webpack --mode=development --watch",
    "server:watch": "nodemon -w ./dist/server.js -w .server.env -r dotenv/config ./dist/server.js dotenv_config_path=.server.env",
    "runner:watch": "nodemon -w ./dist/runner.js -w .runner.env -r dotenv/config ./dist/runner.js dotenv_config_path=.runner.env",
    "test": "jest --runInBand --coverage --no-cache --silent",
    "lint": "eslint ./src --ext .ts,.tsx",
    "lint:fix": "eslint ./src --ext .js,.jsx,.ts,.tsx --fix",
    "semantic-release": "semantic-release"
  },
